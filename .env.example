# AI SDK Provider (zai | google | openrouter | anthropic | openai)
AI_SDK_PROVIDER=zai

# Z.AI (Recommended - Default Provider)
ZAI_API_KEY=
ZAI_MODEL=glm-4.6

# Google Gemini (if provider=google)
# Get your API key at: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=

# OpenRouter (if provider=openrouter)
OPENROUTER_API_KEY=
OPENROUTER_MODEL=openrouter/auto

# Anthropic (if provider=anthropic)
ANTHROPIC_API_KEY=

# OpenAI (if provider=openai)
OPENAI_API_KEY=

# Next.js Configuration
NODE_ENV=development

# Development Settings
NEXT_TELEMETRY_DISABLED=1

# Feature flags (legacy)
# Kept for compatibility; not required now.
AI_SDK_ENABLED=false
GEMINI_ENABLE_SEARCH=false

# Upstash Redis (opcional para registry de MCP)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Clerk Authentication (opcional - funciona em modo keyless se vazio)
# Get your keys at: https://clerk.com
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# Supabase (opcional - usa localStorage se vazio)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
